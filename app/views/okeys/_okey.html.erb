<% if current_user.already_okeyed?(event) %>
  <%= link_to event_okey_path(event.id, current_user.id), method: :delete, remote: true do %>
    <i class="fas fa-check-square fa-lg"></i>
  <% end %>
<% else %>
  <%= link_to event_okeys_path(event.id, current_user.id), method: :post, remote: true do %>
    <i class="far fa-check-square fa-lg"></i>
  <% end %>
<% end %>