<h4 class="mt-3 text-center mt-5">予定登録</h4>
  <%= form_with model: event, url: events_path, local: true, class: "mb-3 pl-md-3" do |f| %>
  <div class="form-group">
    <%= f.label "タイトル", class: "text-info" %>
    <div class="field">
      <%= f.text_field :title, class:"form-control" %>
    </div>
  </div>
  <div class="form-group">
      <%= f.label "開始日時", class: "text_info" %>
       <div class="field">
      <%= f.datetime_field :start_time, class: "form_controll" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label "終了日時", class: "text_info" %>
    <div class="field">
      <%= f.datetime_field :finish_time, class: "form_controll" %>
    </div>
  </div>
  <div class="form-group">
    <div class="field">
      <%= f.label "概要", class: "text-info" %>
      <%= f.text_area :overview, class: "form-control" %>
    </div>
  </div>
  <div class="form_group">
     <%= f.submit "登録する", class: "btn btn-info btn-md" %>
  </div>
   <% end %>